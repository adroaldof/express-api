  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
    "lint": "eslint 'src/**/*.ts'",
    "prettier": "prettier --write 'src/**/*.ts'",
    "start": "tsc && node dist/server.js",
